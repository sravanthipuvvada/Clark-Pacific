{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "social-media-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/socialMedia/SocialMediaWebPart.js",
          "manifest": "./src/webparts/socialMedia/SocialMediaWebPart.manifest.json"
        }
      ]
    },
    "tech-advocates-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/techAdvocates/TechAdvocatesWebPart.js",
          "manifest": "./src/webparts/techAdvocates/TechAdvocatesWebPart.manifest.json"
        }
      ]
    },
    "employee-spotlight-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeSpotlight/EmployeeSpotlightWebPart.js",
          "manifest": "./src/webparts/employeeSpotlight/EmployeeSpotlightWebPart.manifest.json"
        }
      ]
    },
    "project-search-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectSearch/ProjectSearchWebPart.js",
          "manifest": "./src/webparts/projectSearch/ProjectSearchWebPart.manifest.json"
        }
      ]
    },
    "script-editor-bundle": {
      "components": [
        {
          "entrypoint": "./lib/webparts/scriptEditor/ScriptEditorWebPart.js",
          "manifest": "./src/webparts/scriptEditor/ScriptEditorWebPart.manifest.json"
        }
      ]
    },
    "work-anniversary-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/workAnniversary/WorkAnniversaryWebPart.js",
          "manifest": "./src/webparts/workAnniversary/WorkAnniversaryWebPart.manifest.json"
        }
      ]
    },
    "my-projects-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myProjects/MyProjectsWebPart.js",
          "manifest": "./src/webparts/myProjects/MyProjectsWebPart.manifest.json"
        }
      ]
    },
    "my-favorites-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myFavorites/MyFavoritesWebPart.js",
          "manifest": "./src/webparts/myFavorites/MyFavoritesWebPart.manifest.json"
        }
      ]
    },
    "project-site-creation-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/projectSiteCreation/ProjectSiteCreationWebPart.js",
          "manifest": "./src/webparts/projectSiteCreation/ProjectSiteCreationWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.min.js",
      "globalName": "jQuery"
    },
    "twitter": {
      "path": "https://platform.twitter.com/widgets.js",
      "globalName": "twttr"
    },
    "socialstream": {
      "path": "src/javascripts/socialStream/socialstream.jquery.js",
      "globalName": "jQuery",
      "globalDependencies": [
        "jquery"
      ]
    }
  },
  "localizedResources": {
    "SocialMediaWebPartStrings": "lib/webparts/socialMedia/loc/{locale}.js",
    "TechAdvocatesWebPartStrings": "lib/webparts/techAdvocates/loc/{locale}.js",
    "EmployeeSpotlightWebPartStrings": "lib/webparts/employeeSpotlight/loc/{locale}.js",
    "ProjectSearchWebPartStrings": "lib/webparts/projectSearch/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "WorkAnniversaryWebPartStrings": "lib/webparts/workAnniversary/loc/{locale}.js",
    "MyProjectsWebPartStrings": "lib/webparts/myProjects/loc/{locale}.js",
    "MyFavoritesWebPartStrings": "lib/webparts/MyFavorites/loc/{locale}.js",
    "ProjectSiteCreationWebPartStrings": "lib/webparts/projectSiteCreation/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}